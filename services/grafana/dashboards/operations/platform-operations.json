          "rawSQL": "SELECT d.driver_id, d.first_name || ' ' || d.last_name as driver_name,\n  COUNT(t.trip_key) as total_trips, AVG(r.rating) as avg_rating,\n  CAST(SUM(t.fare) AS DECIMAL(10,2)) as total_earnings, AVG(t.distance_km) as avg_distance\nFROM delta.gold.fact_trips t\nJOIN delta.gold.dim_drivers d ON t.driver_key = d.driver_key AND d.current_flag = TRUE\nLEFT JOIN delta.gold.fact_ratings r ON t.trip_key = r.trip_key AND r.ratee_type = 'driver'\nWHERE t.completed_at >= current_timestamp - INTERVAL '24' HOUR AND t.trip_state = 'completed'\nGROUP BY d.driver_id, d.first_name, d.last_name ORDER BY total_trips DESC LIMIT 20",
          "refId": "A"
        }

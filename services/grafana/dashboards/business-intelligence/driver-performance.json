          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  d.driver_id,\n  d.dna_acceptance_rate * 100 as dna_acceptance,\n  AVG(CASE WHEN t.trip_status IN ('completed', 'cancelled') THEN 1.0 ELSE 0.0 END) * 100 as actual_acceptance,\n  COUNT(t.trip_id) as trip_count\nFROM delta.gold.driver_dim d\nJOIN delta.gold.trips_fact t ON d.driver_id = t.driver_id\nWHERE t.completed_at >= current_timestamp - INTERVAL '7' DAY\nGROUP BY d.driver_id, d.dna_acceptance_rate\nHAVING COUNT(t.trip_id) >= 10\nORDER BY trip_count DESC\nLIMIT 100",
          "refId": "A"
        }

          "format": 0,
          "rawQuery": true,
          "rawSQL": "SELECT\n  CASE\n    WHEN hour(requested_at) BETWEEN 7 AND 9 THEN 'Morning Peak (7-9)'\n    WHEN hour(requested_at) BETWEEN 17 AND 19 THEN 'Evening Peak (17-19)'\n    ELSE 'Off-Peak'\n  END as time_period,\n  COUNT(*) as trip_count,\n  AVG(date_diff('second', requested_at, matched_at)) as avg_wait\nFROM delta.gold.fact_trips\nWHERE requested_at >= current_timestamp - INTERVAL '7' DAY\nGROUP BY\n  CASE\n    WHEN hour(requested_at) BETWEEN 7 AND 9 THEN 'Morning Peak (7-9)'\n    WHEN hour(requested_at) BETWEEN 17 AND 19 THEN 'Evening Peak (17-19)'\n    ELSE 'Off-Peak'\n  END\nORDER BY\n  CASE time_period\n    WHEN 'Morning Peak (7-9)' THEN 1\n    WHEN 'Evening Peak (17-19)' THEN 2\n    ELSE 3\n  END",
          "refId": "A"
        }
      ],

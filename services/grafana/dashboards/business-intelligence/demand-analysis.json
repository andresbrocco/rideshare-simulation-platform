          "format": 0,
          "rawQuery": true,
          "rawSql": "SELECT\n  CASE\n    WHEN date_diff('minute', requested_at, matched_at) < 2 THEN '0-2 min'\n    WHEN date_diff('minute', requested_at, matched_at) < 5 THEN '2-5 min'\n    WHEN date_diff('minute', requested_at, matched_at) < 10 THEN '5-10 min'\n    WHEN date_diff('minute', requested_at, matched_at) < 15 THEN '10-15 min'\n    ELSE '15+ min'\n  END as wait_bucket,\n  COUNT(*) as trip_count\nFROM delta.gold.fact_trips\nWHERE requested_at >= current_timestamp - INTERVAL '7' DAY\n  AND matched_at IS NOT NULL\nGROUP BY\n  CASE\n    WHEN date_diff('minute', requested_at, matched_at) < 2 THEN '0-2 min'\n    WHEN date_diff('minute', requested_at, matched_at) < 5 THEN '2-5 min'\n    WHEN date_diff('minute', requested_at, matched_at) < 10 THEN '5-10 min'\n    WHEN date_diff('minute', requested_at, matched_at) < 15 THEN '10-15 min'\n    ELSE '15+ min'\n  END\nORDER BY\n  CASE wait_bucket\n    WHEN '0-2 min' THEN 1\n    WHEN '2-5 min' THEN 2\n    WHEN '5-10 min' THEN 3\n    WHEN '10-15 min' THEN 4\n    ELSE 5\n  END",
          "refId": "A"
        }
      ],

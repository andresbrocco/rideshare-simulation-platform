          },
          "format": 0,
          "rawQuery": true,
          "rawSQL": "WITH all_trips AS (\n  SELECT trip_id, requested_at\n  FROM delta.gold.fact_trips\n  WHERE requested_at >= current_timestamp - INTERVAL '7' DAY\n  UNION ALL\n  SELECT trip_id, requested_at\n  FROM delta.gold.fact_cancellations\n  WHERE requested_at >= current_timestamp - INTERVAL '7' DAY\n),\ncompleted AS (\n  SELECT COUNT(*) as cnt\n  FROM delta.gold.fact_trips\n  WHERE requested_at >= current_timestamp - INTERVAL '7' DAY\n)\nSELECT\n  CASE WHEN COUNT(*) = 0 THEN 0.0\n    ELSE CAST((SELECT cnt FROM completed) AS DOUBLE) * 100.0 / CAST(COUNT(*) AS DOUBLE)\n  END as completion_rate\nFROM all_trips",
          "refId": "A"
        }
      ],
      "title": "Completion Rate %",
      "type": "stat"

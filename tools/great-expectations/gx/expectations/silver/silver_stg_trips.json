{
  "expectation_suite_name": "silver_stg_trips",
  "data_asset_type": "Dataset",
  "expectations": [
    {
      "expectation_type": "expect_column_values_to_be_unique",
      "kwargs": {
        "column": "event_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "event_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "trip_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "timestamp"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "trip_state",
        "value_set": ["requested", "offer_sent", "offer_expired", "offer_rejected", "matched", "driver_en_route", "driver_arrived", "started", "completed", "cancelled"]
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "surge_multiplier",
        "min_value": 1.0,
        "max_value": 2.5,
        "mostly": 0.99
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "fare",
        "min_value": 0.0,
        "mostly": 0.95
      }
    }
  ]
}
